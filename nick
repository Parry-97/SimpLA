s: string;
i:boolean;

func funzione(str: string, num: integer): void
j:integer;
body
    if str < "ciao" then
        writeln("ok");
    else
        writeln(str);
    end;

    for j = 0 to num do
        write(j);
    end;
end;

body
    write("leggi una stringa ");
    read(s);
    write("leggi un boolean ");
        read(i);
        writeln("i: ",i);
    funzione(s,13);
end.

