y,z: vector [5] of integer;
x: vector[2] of vector[5] of integer;

i,j, k: integer;

#Test funzioni e vettori

func copyvet(param: vector[5] of integer): vector [5] of integer
testi: integer;
retvec: vector [5] of integer;
body
    for k = 0 to 4 do
        retvec[k] = param[k] + 1;
    end;
    
    # Modifichiamo il parametro formale
    for k = 0 to 4 do
        param[k] = 11;
    end;
    

    return retvec;
end;

body
    z = vector(1,2,3,4,5);
    x = vector(vector(1,2,3,4,5),copyvet(z));


    #la variabile z non cambia 
    for k = 0 to 4 do
        writeln("z[",k,"]: ", z[k]);
    end;

    # la matrice x risultato della concatenazione tra z e copyvet(z)
    for i = 0 to 1 do
        for j  = 0 to 4 do
            writeln("elemento x[",i,"][",j,"] Ã¨ ", x[i][j]);
        end;
    end;
end.